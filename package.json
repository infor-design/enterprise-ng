{
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --browsers=PhantomJS --code-coverage",
    "lint": "ng lint && npm run mdlint",
    "mdlint": "npm run mdlint:docs && npm run mdlint:src",
    "mdlint:src": "npx markdownlint src",
    "mdlint:docs": "npx markdownlint docs/ && npx markdownlint README.md",
    "e2e": "ng e2e",
    "call-prepublishOnly": "cd ./publish && npm run prepublishOnly && cd ../",
    "update-enterprise": "node ./scripts/update-ids-enterprise.js",
    "version-bump:dev": "node ./scripts/version-append-date",
    "release:dev": "npm run update-enterprise && npm run version-bump:dev && npm publish publish/ --tag=dev",
    "release:beta": "cd publish/ && release-it minor --preRelease=beta",
    "release:rc": "cd publish/ && release-it --preRelease=rc",
    "release:final": "cd publish/ && release-it"
  },
  "dependencies": {
    "@angular/animations": "6.0.4",
