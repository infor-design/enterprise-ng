{
  "name": "ids-enterprise-ng-app",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=10000 ./node_modules/@angular/cli/bin/ng",
    "ng": "npx ng",
    "start": "npm run ng-high-memory -- serve",
    "build": "npm run build:lib && npm run build:app",
    "build:lib": "npm run build:typings && npm run build:library",
    "build:library": "node --max_old_space_size=10000 ./node_modules/@angular/cli/bin/ng build ids-enterprise-ng --configuration production",
    "build:typings": "node ./scripts/build-ids-enterprise-typings.js",
    "build:app": "node --max_old_space_size=108000 ./node_modules/@angular/cli/bin/ng build",
    "pack:lib": "cd dist/ids-enterprise-ng && npm pack",
    "package": "npm run build:lib && npm pack",
    "start-server": "npm run start",
    "test:ci": "npm run test",
    "test:ci:percy": "npm run test:percy",
    "test:percy": "npx percy exec -- npx playwright test --reporter=list --workers=1 --grep 'percy'",
    "test:lib": "npx ng test ids-enterprise-ng --code-coverage --watch=false",
    "test": "npx playwright test --reporter=list --workers=3 --grep-invert 'percy'",
    "lint": "npx ng lint && npm run mdlint",
    "mdlint": "npm run mdlint:docs && npm run mdlint:src",
    "mdlint:src": "npx markdownlint ./projects/ids-enterprise-ng/src/",
    "mdlint:docs": "npx markdownlint docs/ && npx markdownlint README.md",
    "update-enterprise": "node ./scripts/update-ids-enterprise.js && npm run sync:lib",
    "version-bump:dev": "node ./scripts/version-append-date",
    "release:final": "npm run sync:lib && npm run build:lib && cd dist/ids-enterprise-ng && npm publish && cd ../../ && npm run release:typings",
    "release:typings": "npm run sync:lib && npm run build:typings && cd dist/ids-enterprise-typings && npm publish",
    "sync:lib": "node ./scripts/version-sync.js"
  },
  "private": true,
  "type": "module",
  "peerDependencies": {
    "@angular/animations": "^18.2.11",
    "@angular/common": "^18.2.11",
    "@angular/compiler": "^18.2.11",
    "@angular/core": "^18.2.11",
    "@angular/forms": "^18.2.11",
    "@angular/platform-browser": "^18.2.11",
    "@angular/platform-browser-dynamic": "^18.2.11",
    "@angular/router": "^18.2.11",
    "d3": "7.9.0",
    "ids-enterprise": "^4.101.0",
    "jquery": "^3.7.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular-eslint/builder": "~18.2.0",
    "@angular-eslint/eslint-plugin": "~17.5.1",
    "@angular-eslint/eslint-plugin-template": "~17.5.1",
    "@angular-eslint/template-parser": "~17.5.1",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.11",
    "@angular/language-service": "^18.2.11",
    "@axe-core/playwright": "^4.10.0",
    "@percy/cli": "^1.29.3",
    "@percy/playwright": "^1.0.6",
    "@percy/sdk-utils": "^1.29.3",
    "@playwright/test": "^1.47.0",
    "@types/d3": "7.4.3",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/jquery": "3.5.30",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "del": "^7.1.0",
    "eslint": "^8.55.0",
    "fs-extra": "^11.2.0",
    "htmlhint": "^1.1.4",
    "inquirer": "^10.2.2",
    "karma": "~6.4.4",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "kill-port": "^2.0.1",
    "markdownlint-cli": "^0.41.0",
    "merge-stream": "^2.0.0",
    "ng-packagr": "^18.2.1",
    "puppeteer": "^22.15.0",
    "slash": "^5.1.0",
    "ts-node": "~10.9.2",
    "typescript": "^5.5.4",
    "yargs": "^17.7.2"
  }
}
